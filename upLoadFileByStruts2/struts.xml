<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	
	<package name="bookMgr" extends="struts-default" namespace="/book">
		
		<action name="saveBookForm">
			<result name="success">/WEB-INF/jsp/saveBook.jsp</result>
		</action>
	
		<action name="saveBook" class="com.wuchanglin.web.controller.BookMgr" method="saveBook">
			<result name="success">/WEB-INF/jsp/saveBook.jsp</result>
			<result name="input">/WEB-INF/jsp/saveBook.jsp</result>
		</action>
	</package>

</struts>
